package Pckage_1;

public class loans {

	public void loans_TC_1() {

		System.out.println("This is TC 1 from loans");

	}

	public void loans_TC_2() {

		System.out.println("This is TC 2 from loans");

	}

	public void loans_TC_3() {

		System.out.println("This is TC 3 from loans");

	}

	public void loans_TC_4() {

		System.out.println("This is TC 4 from loans");

	}

	public void loans_TC_5() {

		System.out.println("This is TC 5 from loans");

	}

	public void loans_TS_1() {

		loans_TC_1();
		loans_TC_2();
		loans_TC_3();

	}

	public void loans_TS_2() {

		loans_TC_4();
		loans_TC_5();

	}

	public static void main(String[] args) {

	}

}
